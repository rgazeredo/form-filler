{
  "manifest_version": 3,
  "name": "Form Filler - Preenchedor Automático",
  "author": "Raphael Azeredo",
  "email": "rgazeredo@gmail.com",
  "homepage_url": "https://github.com/rgazeredo/form-filler",
  "version": "1.0.0",
  "description": "Preenche automaticamente formulários para teste com dados simulados",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
